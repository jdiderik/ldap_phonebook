{
  "name": "phonebook",
  "displayName": "Phonebook",
  "version": "1.0.0",
  "description": "Phonebook",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel serve src/index.html --no-cache --dist-dir ./.dev/",
    "prebuild": "rm -rf ./www/* && rm -rf ./.parcel-cache/*",
    "build": "NODE_ENV=production parcel build src/index.html --no-source-maps --dist-dir ./www/ --public-url ./",
    "prebuild:debug": "rm -rf ./www/* && rm -rf ./.parcel-cache/*",
    "build:debug": "parcel build src/index.html --no-optimize --dist-dir ./www/ --public-url ./"
  },
