    "construct-ui": "^0.3.4",
    "dexie": "^4.2.1",
    "mithril": "^2.3.8"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.10.3",
    "@parcel/packager-xml": "^2.10.3",
    "@parcel/transformer-sass": "^2.10.3",
    "@parcel/transformer-webmanifest": "^2.10.3",
    "parcel": "^2.10.3",
    "parcel-config-pwa-manifest": "^0.1.6"
  }
}
